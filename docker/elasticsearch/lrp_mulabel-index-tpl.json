{
  "index_patterns": [
    "lrp_mulabel*", "lrp_eurlex*", "lrp_20news*", "lrp_reuters*"
  ],
  "priority": 10,
  "template" : {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0
    },
    "mappings": {
      "_source": { "enabled": true },
      "properties": {
        "uuid": { "type": "keyword"},
        "a_uuid": { "type": "keyword"},
        "a_id": {"type": "keyword"},
        "country": {"type": "keyword"},
        "lang": {"type": "keyword"},
        "m_id": {"type": "keyword"},
        "label_info": {
          "type": "nested",
          "properties": {
            "id": {"type": "keyword"},
            "name": {"type": "text"},
            "kwe": {
              "type": "nested",
              "properties": {
                "id": {"type": "keyword"},
                "value": {"type": "text"}
              }
            }
          }
        },
        "label": {"type": "keyword"},
        "passage_cat": {"type": "long"},
        "passage_targets": {"type": "long"},
        "m_social": {"type": "long"},
        "dup": {"type": "long"},
        "text": {"type": "text"},
        "published": {"type": "date"},
        "created": {"type": "date"},
        "date": {"type": "date"},
        "m_bge_m3": {
          "type": "dense_vector",
          "dims": 1024,
          "similarity": "dot_product"
        }
      }
    }
  }
}